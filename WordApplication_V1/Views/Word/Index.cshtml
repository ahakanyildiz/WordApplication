@model List<Word>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<table>
    <thead>
        <tr>
            <td>Id</td>
            <td>En Name</td>
            <td>Tr Name</td>
            <td>Image</td>
        </tr>
    </thead>

    <tbody>
        @foreach(var item in Model)
        {
            <tr>    
                <td>@item.Id</td>
                <td>@item.EnName</td>
                <td>@item.TrName</td>
                <td>@item.ImageUrl</td>
            </tr>
        }
    </tbody>
</table>



